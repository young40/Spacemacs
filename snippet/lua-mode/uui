function `(file-name-nondirectory (file-name-sans-extension (buffer-file-name)))`:OnInit(param)
end

function `(file-name-nondirectory (file-name-sans-extension (buffer-file-name)))`:Update()
end

function `(file-name-nondirectory (file-name-sans-extension (buffer-file-name)))`:OnEnable()
end

function `(file-name-nondirectory (file-name-sans-extension (buffer-file-name)))`:OnDisable()
